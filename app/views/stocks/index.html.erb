<table>
  <tr>
    <th>Symbol</th>
    <th>Company</th>
    <th></th>
  </tr>
  <% @stocks.each do |stock| %>
    <tr>
      <td>
        <%= stock.symbol %>
      </td>
      <td>
        <%= stock.company %>
      </td>
      <td>
        <%= link_to 'Show', stock_path(stock) %>
      </td>
    </tr>
  <% end %>
</table>
